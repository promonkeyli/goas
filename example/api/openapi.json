{
  "openapi": "",
  "info": {
    "title": "Mall API",
    "description": "Mall API 服务接口文档",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "name": "API Support",
      "url": "http://www.swagger.io/support",
      "email": "support@swagger.io"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8081/"
    }
  ],
  "paths": {
    "/admin/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "登录",
        "description": "用户名/密码登录",
        "operationId": "Login",
        "requestBody": {
          "description": "登录信息",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginReq"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/model.ResponseOfLoginRes"
                }
              }
            }
          }
        }
      }
    },
    "/users": {
      "get": {
        "tags": [
          "user"
        ],
        "summary": "获取用户列表",
        "description": "分页查询用户列表",
        "operationId": "ListUsers",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "页码",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "size",
            "in": "query",
            "description": "每页数量",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          },
          {
            "name": "keyword",
            "in": "query",
            "description": "搜索关键词",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/model.ResponseOfmodel.PageListOfUser"
                }
              }
            }
          },
          "400": {
            "description": "参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "user"
        ],
        "summary": "创建用户",
        "description": "创建一个新用户",
        "operationId": "CreateUser",
        "requestBody": {
          "description": "用户信息",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "创建成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/model.ResponseOfUser"
                }
              }
            }
          },
          "400": {
            "description": "参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/users/{id}": {
      "put": {
        "tags": [
          "user"
        ],
        "summary": "更新用户信息",
        "description": "更新指定用户的信息",
        "operationId": "UpdateUser",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "用户ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "description": "用户信息",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "更新成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/model.ResponseOfUser"
                }
              }
            }
          },
          "400": {
            "description": "参数错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "404": {
            "description": "用户不存在",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "user"
        ],
        "summary": "删除用户",
        "description": "删除指定用户",
        "operationId": "DeleteUser",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "用户ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "204": {
            "description": ""
          },
          "404": {
            "description": "用户不存在",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        },
        "deprecated": true
      }
    },
    "/users/{id}/avatar": {
      "post": {
        "tags": [
          "user"
        ],
        "summary": "上传用户头像",
        "operationId": "UploadAvatar",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "用户ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "avatar": {
                    "description": "头像文件",
                    "type": "string",
                    "format": "binary"
                  }
                },
                "required": [
                  "avatar"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "上传成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/model.ResponseOfstring"
                }
              }
            }
          },
          "400": {
            "description": "上传失败",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ErrorResponse": {
        "title": "ErrorResponse",
        "type": "object",
        "properties": {
          "code": {
            "description": "错误码",
            "type": "integer",
            "format": "int32"
          },
          "details": {
            "description": "详细信息",
            "type": "string"
          },
          "message": {
            "description": "错误信息",
            "type": "string"
          }
        },
        "required": [
          "code",
          "message"
        ]
      },
      "LoginReq": {
        "title": "LoginReq",
        "type": "object",
        "properties": {
          "password": {
            "description": "密码",
            "type": "string"
          },
          "username": {
            "description": "用户名",
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ]
      },
      "LoginRes": {
        "title": "LoginRes",
        "type": "object",
        "properties": {
          "access_token": {
            "description": "访问凭证",
            "type": "string"
          },
          "refresh_token": {
            "description": "刷新凭证",
            "type": "string"
          }
        },
        "required": [
          "access_token",
          "refresh_token"
        ]
      },
      "User": {
        "title": "User",
        "type": "object",
        "properties": {
          "age": {
            "description": "年龄",
            "type": "integer",
            "format": "int32"
          },
          "email": {
            "description": "邮箱",
            "type": "string"
          },
          "id": {
            "description": "用户ID",
            "type": "integer",
            "format": "int32"
          },
          "is_active": {
            "description": "是否激活",
            "type": "boolean"
          },
          "name": {
            "description": "用户名",
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "is_active"
        ]
      },
      "model.PageListOfUser": {
        "title": "model.PageListOfUser",
        "type": "object",
        "properties": {
          "items": {
            "description": "数据列表",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/User"
            }
          },
          "page": {
            "description": "当前页",
            "type": "integer",
            "format": "int32"
          },
          "size": {
            "description": "每页数量",
            "type": "integer",
            "format": "int32"
          },
          "total": {
            "description": "总数",
            "type": "integer",
            "format": "int32"
          }
        },
        "required": [
          "total",
          "page",
          "size",
          "items"
        ]
      },
      "model.ResponseOfLoginRes": {
        "title": "model.ResponseOfLoginRes",
        "type": "object",
        "properties": {
          "code": {
            "description": "状态码",
            "type": "integer",
            "format": "int32"
          },
          "data": {
            "$ref": "#/components/schemas/LoginRes",
            "description": "数据"
          },
          "message": {
            "description": "消息",
            "type": "string"
          }
        },
        "required": [
          "code",
          "message",
          "data"
        ]
      },
      "model.ResponseOfUser": {
        "title": "model.ResponseOfUser",
        "type": "object",
        "properties": {
          "code": {
            "description": "状态码",
            "type": "integer",
            "format": "int32"
          },
          "data": {
            "$ref": "#/components/schemas/User",
            "description": "数据"
          },
          "message": {
            "description": "消息",
            "type": "string"
          }
        },
        "required": [
          "code",
          "message",
          "data"
        ]
      },
      "model.ResponseOfmodel.PageListOfUser": {
        "title": "model.ResponseOfmodel.PageListOfUser",
        "type": "object",
        "properties": {
          "code": {
            "description": "状态码",
            "type": "integer",
            "format": "int32"
          },
          "data": {
            "$ref": "#/components/schemas/model.PageListOfUser",
            "description": "数据"
          },
          "message": {
            "description": "消息",
            "type": "string"
          }
        },
        "required": [
          "code",
          "message",
          "data"
        ]
      },
      "model.ResponseOfstring": {
        "title": "model.ResponseOfstring",
        "type": "object",
        "properties": {
          "code": {
            "description": "状态码",
            "type": "integer",
            "format": "int32"
          },
          "data": {
            "description": "数据",
            "type": "string"
          },
          "message": {
            "description": "消息",
            "type": "string"
          }
        },
        "required": [
          "code",
          "message",
          "data"
        ]
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      },
      "CookieAuth": {
        "type": "apiKey",
        "name": "Cookie",
        "in": "header"
      }
    }
  }
}